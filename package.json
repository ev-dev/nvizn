  "main": "src/index.js",
  "scripts": {
    "dev": "npm run serve-dev & npm run build-dev",
    "build-dev": "cross-env NODE_ENV=development webpack -w",
    "serve-dev": "nodemon -r @std/esm ./src/server/index.js --watch server",
