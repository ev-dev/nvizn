    "test-db": "nodemon tests/dbTestServer.js --watch tests",
    "test": "nodemon tests/testServer.js --watch tests",
    "start": "npm run build && npm run serve",
    "build": "cross-env NODE_ENV=production webpack",
    "serve": "cross-env NODE_ENV=production node ./src/server/index.js",
    "seed": "node -r @std/esm ./src/server/config/seed.js"
  },
  "keywords": [
    "arxiv",
    "research",
    "science",
    "aggregator"
  ],
  "author": "ev-dev",
  "license": "MIT",
